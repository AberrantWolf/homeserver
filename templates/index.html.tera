{% extends "base" %}

{% block content %}
    <h1>Hi {{name}}</h1>
    <h3>Here are your items:</h3>
    <ul>
        {% for s in items %}
            <li>{{ s }}</li>
        {% endfor %}
    </ul>

    Your cookie message is: {{ cookie_msg }}

    <p>Try going to <a href="/hello/YourName">/hello/YourName</a></p>
{% endblock content %}